{
   "name": "node-mongo-boilerplate",
   "version": "1.0.0",
   "description": "A boilerplate for node.js, TypeScript and MongoDB. Built with ❤️  by @bkoiki950",
   "main": "index.js",
   "scripts": {
      "test": "NODE_ENV=test mocha dist/src/shared/test_utils/before_hook --exit --timeout 10000 --bail --reporter spec --exit --full-trace --sort --retries 2 dist/**/*.spec.js",
      "lint": "node dist/src/lint_codes.js",
      "build": "tsc --project ./tsconfig.json",
      "start": "node dist/src/index.js",
      "build:watch": "tsc --project ./tsconfig.json --watch",
      "dev": "nodemon dist/src/index.js",
      "migrate:up": "node dist/src/migrations/index.js",
      "prepare": "husky install"
   },
   "keywords": [],
   "author": "Babatunde Koiki",
   "license": "ISC",
   "dependencies": {
      "axios": "0.24.0",
      "bcrypt": "5.0.1",
      "dotenv": "12.0.4",
      "express": "4.17.2",
      "glob": "7.2.0",
      "jsonwebtoken": "8.5.1",
      "logform": "2.6.0",
      "mongoose": "6.1.6",
      "morgan": "1.10.0",
      "redis": "^4.6.13",
      "validator": "13.11.0",
      "winston": "3.12.0",
      "winston-mongodb": "5.1.1",
      "winston-transport-sentry-node": "2.7.0"
   },
   "devDependencies": {
      "@types/axios": "0.14.0",
      "@types/bcrypt": "5.0.0",
      "@types/chai": "4.3.0",
      "@types/dotenv": "8.2.0",
      "@types/eslint": "8.56.6",
      "@types/express": "4.17.13",
      "@types/glob": "7.2.0",
      "@types/jsonwebtoken": "8.5.7",
      "@types/mocha": "9.0.0",
      "@types/mongoose": "5.11.97",
      "@types/morgan": "1.9.3",
      "@types/node": "17.0.8",
      "@types/sinon": "10.0.13",
      "@types/supertest": "2.0.12",
      "@types/validator": "13.11.9",
      "@typescript-eslint/eslint-plugin": "5.9.1",
      "@typescript-eslint/parser": "5.9.1",
      "chai": "4.3.4",
      "eslint": "8.6.0",
      "husky": "7.0.4",
      "mocha": "9.1.4",
      "nock": "^13.5.4",
      "nodemon": "2.0.15",
      "sinon": "15.0.1",
      "superagent": "7.0.2",
      "supertest": "6.3.3",
      "typescript": "4.5.4"
   }
}
